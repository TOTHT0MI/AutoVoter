{
  "manifest_version": 2,

  "name": "Auto Voter",
  "description": "Helps you vote on your favorite Minecraft server!",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "run_at" :"document_end",
      "matches": [
        "https://minecraft-server-list.com/*",
        "https://minecraft-mp.com/*",
        "https://minecraftservers.biz/*",
        "https://minecraft-server.net/*"
      ],
      "js": ["jquery-3.6.0.min.js","injected.js"]
    }
  ]
}
